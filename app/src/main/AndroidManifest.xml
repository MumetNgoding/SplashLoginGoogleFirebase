<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="founder.nugroho.splashlogingooglefirebase">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="New Login Page"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".TypeFile"/>
        <activity android:name=".FullCostType"/>
        <activity android:name=".OtherAskType"/>
        <activity android:name=".PeopleWantedType"/>
    </application>

</manifest>